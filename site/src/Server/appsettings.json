{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApplicationInsights": {
    "ConnectionString": "%APPLICATIONINSIGHTS_CONNECTION_STRING%",
    "EnableAdaptiveSampling": false
  },
  "ProjectsDatabase": {
    "ConnectionString": "%PROJECTS_DATABASE_CONNECTIONSTRING%",
    "DatabaseName": "%PROJECTS_DATABASE_DATABASE_NAME%",
    "CollectionName": "%PROJECTS_DATABASE_COLLECTION_NAME%"
  }
}
